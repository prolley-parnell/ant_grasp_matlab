        <parent link="left_jaw_base"/>
        <child link="left_jaw"/>
    </joint>
        <joint name="fixed_left_2" type="fixed">
        <origin xyz="0.0 0.0 0.3" rpy="1.3 0.0 0.0"/>

        <parent link="left_jaw"/>
        <child link="left_jaw_tip"/>
    </joint>

        <joint name="fixed_right_1" type="fixed">
